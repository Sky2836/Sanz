	   wait(2)
	   game:GetService("VirtualUser"):SetKeyUp('0x65')
	end
end)

Farm_Haki:Button("Check Observation Level", function()
    while game:GetService("RunService").Heartbeat:wait() do
        if game:GetService("Players").LocalPlayer.VisionRadius.Value == 5000 then
            Value = "Level Max"
            else
                Value = math.round(game:GetService("Players").LocalPlayer.VisionRadius.Value)
        end
    end
    Blacklib:Notification("Observation Info", "Level Observation : "..tostring(Value))
end)

