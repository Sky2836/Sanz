-- Quest_1
if Old_World then
	Quest_1:Toggle("Auto Second Sea", function(vu)
	    _G.AutoNewworld = vu
		Auto_Newworld = vu
		if vu == false then
			wait(1)
			TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		end
	end)
end

if New_World then
	Quest_1:Toggle("Auto Third Sea", function(vu)
	    _G.AutoThirdSea = vu
        ReadyThirdSea = vu
		TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        if ReadyThirdSea and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") ~= 3 then
            Blacklib:Notification("Auto Third Sea", "Finish Bartilo Quest First.")
        elseif ReadyThirdSea and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ZQuestProgress","Check") ~= 0 then
            Blacklib:Notification("Auto Third Sea", "u must have to Killed Don Swan First")
        elseif ReadyThirdSea and SelectToolWeapon == "" then
            Blacklib:Notification("Notice..!", "Select Weapon First")
        else
            AutoThird = vu
        end
    end)
end

