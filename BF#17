    Quest_2:Toggle("Auto Farm Factory", function(vu)
        Factory = vu
		if vu == false then
			wait(1)
			TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		end
    end)
end

if Three_World then
    Quest_2:Label("")
    CheckElite = Quest_2:Label("")
	Quest_2:Toggle("Auto Elite Hunter", function(vu)
	    _G.AutoElite = vu
		EliteHunter = vu
		if vu == false then
			wait(1)
			TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		end
	end)

	spawn(function()
		while true do
			local TotalElite = tostring(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EliteHunter","Progress"))
			CheckElite:Refresh("Total Elite Hunter Progress : "..TotalElite)
			game:GetService("RunService").Heartbeat:wait()
		end
	end)

	Quest_2:Toggle("Auto Enma/Yama", function(vu)
	    _G.AutoYama = vu
		AutoYama = vu
	end)
	Quest_2:Toggle("Auto Rainbow Haki", function(vu)
		AutoRainbow = vu
		if vu == false then
			wait(1)
			TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		end
	end)
end

if New_World then
	Quest_2:Toggle("Auto Quest Bartilo", function(vu)
		CheckBarto = vu
		if game.Players.LocalPlayer.Backpack:FindFirstChild("Warrior Helmet") then
			Success = true
		elseif game.Players.LocalPlayer.Character:FindFirstChild("Warrior Helmet") then
			Success = true
		end
		if Success and CheckBarto == true then
			Blacklib:Notification("Auto Quest Bartilo", "Successfully")
		elseif CheckBarto == true and MiscFarmWeapon == "" then
			Blacklib:Notification("Notice..!","Please Select Weapon First")
		elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 3 and CheckBarto == true then
			Blacklib:Notification("Auto Quest Bartilo", "Successfully")
		else
			AutoBartilo = vu
			if vu == false then
				wait(1)
				TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
			end
		end
	end)

	Quest_2:Toggle("Auto Evo Race", function(vu)
		CheckEvo = vu
		if not game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 3 and CheckEvo then
			Blacklib:Notification("Auto Evo Race","You Have To Success Bartilo Quest")
		elseif CheckEvo == true and MiscFarmWeapon == "" then
			Blacklib:Notification("Notice..!","Please Select Weapon First")
		elseif game:GetService("Players").LocalPlayer.Data.Race:FindFirstChild("Evolved") and CheckEvo then
			Blacklib:Notification("Auto Evo Race", "Successfully")
		else
			AutoEvoRace = vu
			if vu == false then
				wait(1)
				TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
			end
		end
	end)

	Quest_2:Toggle("Auto Sword Rengoku", function(vu)
		AutoRengoku = vu
		if vu == false then
			wait(1)
			TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		end
	end)

	Quest_2:Toggle("Auto Ectoplasm", function(vu)
		AutoEcto = vu
		if vu == false then
			wait(1)
			TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		end
	end)
end

if Three_World then
	Quest_2:Toggle("Auto Citizen Quest", function(vu)
		CheckCitizen = vu
		TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		if game.Players.LocalPlayer.Backpack:FindFirstChild("Musketeer Hat") then
			CTCH = true
		elseif game.Players.LocalPlayer.Character:FindFirstChild("Musketeer Hat") then
			CTCH = true
		end
		if CTCH and CheckCitizen then
			Blacklib:Notification("Auto Quest Citizen", "Successfully")
		elseif CheckCitizen and MiscFarmWeapon == "" then
			Blacklib:Notification("Notice..!", "Please Select Weapon First")
		elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CitizenQuestProgress","Citizen") == 3 and CheckCitizen then
			Blacklib:Notification("Auto Quest Citizen", "Successfully")
		else
			AutoCitizen = vu
			if vu == false then
				wait(1)
				TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
			end
		end
	end)
end

