-- Misc_2 (Mods)
Misc_2:Dropdown("Change Buso Stage", {0, 1, 2, 3, 4, 5}, function(valval)
    _G.Vals = valval
end)
spawn(function()
    while wait() do
        if _G.Vals == 0 then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ChangeBusoStage",0)
        elseif _G.Vals == 1 then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ChangeBusoStage",1)
        elseif _G.Vals == 2 then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ChangeBusoStage",2)
        elseif _G.Vals == 3 then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ChangeBusoStage",3)
        elseif _G.Vals == 4 then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ChangeBusoStage",4)
        elseif _G.Vals == 5 then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ChangeBusoStage",5)
        end
    end
end)

Misc_2:Toggle("Auto Agility",function(Value)
		_G.Agility = Value
	end)

Misc_2:Toggle("Walk on Water",function(Value)
	_G.WalkWater = Value
	if _G.WalkWater == true then
		game.Players.LocalPlayer.Data.DevilFruit.Value = ("Ice-Ice")
	elseif _G.WalkWater == false then
		game.Players.LocalPlayer.Data.DevilFruit.Value = ("")
	end
end)

Misc_2:Toggle("Fly", function(Value)
	Fly = Value
	activatefly()
end)

Misc_2:Toggle("No Clip", function(value)
	Clip = value
end)

	
