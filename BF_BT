if Second_Sea then
	Main_Quest_1:Toggle("Auto Quest Bartilo", function(vu)
		CheckBarto = vu
		if game.Players.LocalPlayer.Backpack:FindFirstChild("Warrior Helmet") then
			Success = true
		elseif game.Players.LocalPlayer.Character:FindFirstChild("Warrior Helmet") then
			Success = true
		end
		if Success and CheckBarto == true then
			Blacklib:Notification("Auto Quest Bartilo", "Warrior Helmet Successfully")
		elseif CheckBarto == true and EventWeapon == "" then
			Blacklib:Notification("Auto Quest Bartilo", "Select Weapon First")
		elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 3 and CheckBarto == true then
			Blacklib:Notification("Auto Quest Bartilo", "Bartilo Quest Completed")
		else
			AutoBartilo = vu
			if AutoBartilo == false then
				wait(1)
				TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
			end
		end
	end)

	Main_Quest_1:Toggle("Auto Evo Race", function(vu)
		CheckEvo = vu
		if not game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 3 and CheckEvo then
			Blacklib:Notification("Auto Evo Race","You Need To Success Bartilo Quest")
		elseif CheckEvo == true and EventWeapon == "" then
			Blacklib:Notification("Auto Evo Race", "Select Weapon First")
		elseif game:GetService("Players").LocalPlayer.Data.Race:FindFirstChild("Evolved") and CheckEvo then
			Blacklib:Notification("Auto Evo Race","Evo Race Completed")
		else
			AutoEvoRace = vu
			if AutoEvoRace == false then
				wait(1)
				TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
			end
		end
	end)

	Main_Quest_1:Toggle("Auto Ectoplasm", function(vu)
		AutoEcto = vu
		if AutoEcto == false then
			wait(1)
			TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		end
	end)
end

if Third_Sea then
	Main_Quest_1:Toggle("Auto Citizen Quest", function(vu)
		CheckCitizen = vu
		TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
		if game.Players.LocalPlayer.Backpack:FindFirstChild("Musketeer Hat") then
			CTCH = true
		elseif game.Players.LocalPlayer.Character:FindFirstChild("Musketeer Hat") then
			CTCH = true
		end
		if CTCH and CheckCitizen then
			Blacklib:Notification("Auto Quest Citizen", "Musketeer Hat Successfully")
		elseif CheckCitizen and EventWeapon == "" then
			Blacklib:Notification("Auto Quest Citizen", "Select Weapon First")
		elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CitizenQuestProgress","Citizen") == 3 and CheckCitizen then
			Blacklib:Notification("Auto Quest Citizen", "Citizen Quest Completed")
		else
			AutoCitizen = vu
			if AutoCitizen == false then
				wait(1)
				TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
			end
		end
	end)
end

