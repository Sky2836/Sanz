-- Boss_Farm
if SelectWeaponBoss == nil then
    SelectWeaponBoss = ""
end
local Boss = {}
for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
    if string.find(v.Name, "Boss") then
        if v.Name == "Ice Admiral [Lv. 700] [Boss]" then
		elseif v.Name == "rip_indra [Lv. 1500] [Boss]" then
        else
            table.insert(Boss, v.Name)
        end
    end
end
for i,v in pairs(game.workspace.Enemies:GetChildren()) do
    if string.find(v.Name, "Boss") then
		if v.Name == "Ice Admiral [Lv. 700] [Boss]" then
		elseif v.Name == "rip_indra [Lv. 1500] [Boss]" then
		else
			table.insert(Boss, v.Name)
		end
	end
end

local BossName = Boss_Farm:Dropdown("Select Boss", Boss, function(Value)
    SelectBoss = Value
end)

local BossWeapon = Boss_Farm:Dropdown("Select Weapon",Wapon,function(Value)
    SelectWeaponBoss = Value
end)

Boss_Farm:Button("Refresh Boss", function()
    BossName:Clear()
    for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
        if string.find(v.Name, "Boss") then
            if v.Name == "Ice Admiral [Lv. 700] [Boss]" then
			elseif v.Name == "rip_indra [Lv. 1500] [Boss]" then
			else
                BossName:Add(v.Name)
            end
        end
    end
    for i,v in pairs(game.workspace.Enemies:GetChildren()) do
        if string.find(v.Name, "Boss") then
            if v.Name ~= "Ice Admiral [Lv. 700] [Boss]" or v.Name ~= "rip_indra [Lv. 1500] [Boss]" then
                BossName:Add(v.Name)
            end
        end
    end
end)

Boss_Farm:Button("Refresh Weapon",function()
    BossWeapon:Clear()
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
        if v:IsA("Tool") then
            BossWeapon:Add(v.Name)
        end
    end
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v:IsA("Tool") then
            BossWeapon:Add(v.Name)
        end
    end
end)

Boss_Farm:Toggle("Auto Farm Boss (No Quest)", function(vu)
	AutoBossNoQuest = vu
	if AutoBossNoQuest == false then
		wait(1)
		TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
	end
end)

Boss_Farm:Toggle("Auto Farm Boss (Quest)", function(vu)
	AutoBossQuest = vu
	if AutoBossQuest == false then
	    wait(1)
	    TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
	end
end)

