-- Main Quest
if EventWeapon == nil then
    EventWeapon = ""
end

local SelectWeaponQuest = Main_Quest_1:Dropdown("Select Weapon",Wapon,function(value)
   	EventWeapon = value
end)

Main_Quest_1:Button("Refresh Weapon",function()
    SelectWeaponQuest:Clear()
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v:IsA("Tool") then
			SelectWeaponQuest:Add(v.Name)
		end
	end
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Tool") then
			SelectWeaponQuest:Add(v.Name)
		end
	end
end)

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("getInventoryWeapons")

