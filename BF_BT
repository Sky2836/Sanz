-- Shop_1 (Bone Candy)

Shop_1:Toggle("Random Supprised (50 Bones)", function(x)
	_G.Bone = x
end)
spawn(function()
	 while wait() do
		if _G.Bone then
			local args = {
			[1] = "Bones",
			[2] = "Buy",
			[3] = 1,
			[4] = 1
			}
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
		end
   	end
end)

